---
import Layout from "../../layouts/Layout.astro";
import ContentPageLayout from "../../layouts/ContentPageLayout.astro";
import { getEntry } from "astro:content";

const testing = await getEntry('guides', 'testing');
const { Content } = await testing.render();
---

<Layout title="Reactables - Testing">
  <ContentPageLayout
    editPageLink={{ link: 'https://github.com/reactables/reactables/edit/main/docs/src/content/guides/testng.md'}}
    prevLink={{label: 'Forms', link: '/guides/forms'}}
    nextLink={{label: 'React Bindings', link: '/react/react-bindings'}}
  >
    <Content />
  </ContentPageLayout>

</Layout>