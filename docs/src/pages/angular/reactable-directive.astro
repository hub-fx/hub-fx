---
import Layout from "../../layouts/Layout.astro";
import ContentPageLayout from "../../layouts/ContentPageLayout.astro";
import { getEntry } from "astro:content";

const reactableDirective = await getEntry('angular', 'reactable-directive');
const { Content } = await reactableDirective.render();

const sectionLinks =[
  {
    label: 'Reactable Directive',
    link: '#reactable-directive'
  },
];
---

<Layout title="Reactables - React Bindings">
  <ContentPageLayout
    sectionLinks={sectionLinks}
    editPageLink={{ link: 'https://github.com/reactables/reactables/edit/main/docs/src/content/angular/reactable-directive.md'}}
    prevLink={{label: 'React Form Components', link: '/react/react-form-components'}}
    nextLink={{label: 'Core API', link: '/references/core-api'}}
  >
    <Content />
  </ContentPageLayout>
</Layout>